		System.out.println("Soy el " + Thread.currentThread().getName() + " empezando.");
		for (int i = 0; i < 10; i++)
			System.out.println("Soy el " + Thread.currentThread().getName() + " iteraciÃ³n " + i);
		System.out.println("Soy el " + Thread.currentThread().getName() + " acabando.");
	}
}

